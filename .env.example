# Ejemplo de archivo de variables de entorno
# Copia este archivo a .env y completa los valores

# Base de datos - Supabase PostgreSQL
DATABASE_URL=postgresql://usuario:password@host:puerto/database?pgbouncer=true

# Configuracion alternativa (si no usas DATABASE_URL)
DB_ENGINE=postgresql
DB_NAME=postgres
DB_USER=tu_usuario
DB_PASSWORD=tu_password
DB_HOST=tu_host
DB_PORT=6543

# Django
SECRET_KEY=tu-secret-key-aqui-genera-uno-seguro
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1,.onrender.com,.vercel.app

# Supabase (opcional)
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_KEY=tu-anon-key
